package imp_java_programms;

public class Prim {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num=11,count=1;
		for(int i=1;i<=num;i++) {
			if(num%i == 0)
				count++;
		}
		if(count == 2) {
			System.out.println("this is prime number");
		}
		else
			System.out.println("this is not a prime number");
		}

	}


