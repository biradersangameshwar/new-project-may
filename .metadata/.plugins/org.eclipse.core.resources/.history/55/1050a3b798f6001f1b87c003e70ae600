
public class Reve {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
      String str = "this is beginning";
      int len = str.length();
      char ch[]= str.toCharArray();
      
      for(int i=len-1;i<=0;i--) {
    	  System.out.print(ch[i]);
      }  	  
      }
}