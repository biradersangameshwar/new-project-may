package imp_java_programms;

public class Even_odd {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
         int num = 19;
         if(num%2 == 0) {
        	 System.out.println("this is even number");
         }
        	 else
        		 System.out.println("this is odd number");
         }
	}


