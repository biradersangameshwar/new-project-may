		// TODO Auto-generated method stub
		
		System.out.println("This is beginning");
		
		ArrayList<String> name = new ArrayList<String>();
		System.out.println(name.getClass().getName());
		
	      name.add("sangam");
	      name.add("raju");
	      name.add("hari");
	      name.add("mahesh");
	      
